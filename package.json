{
  "name": "re-svg",
  "version": "0.0.0",
  "description": "A command-line tool for processing and manipulating SVG files.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/author/re-svg#readme",
  "bugs": {
    "url": "https://github.com/author/re-svg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/author/re-svg.git"
  },
  "author": "Author Name <author.name@mail.com>",
  "bin": {
    "re-svg": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown && echo '#!/usr/bin/env node' | cat - dist/index.js > dist/temp.js && mv dist/temp.js dist/index.js && chmod +x dist/index.js",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && bun publish"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "bumpp": "^10.1.0",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "effect": "^3.17.9"
  }
}
